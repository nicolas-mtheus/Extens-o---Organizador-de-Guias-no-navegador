{
    "manifest_version": 3,
    "name": "Organizador de Guias",
    "version": "1.0",
    "description": "Organize suas guias em grupos.",
    "permissions": ["tabs", "tabGroups", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.JPEG",
        "48": "icon48.JPEG",
        "128": "icon128.JPEG"
      }
    },
    "icons": {
      "16": "icon16.JPEG",
      "48": "icon48.JPEG",
      "128": "icon128.JPEG"
    }
    
  }
  